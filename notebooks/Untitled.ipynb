   "source": [
    "import torch\n",
    "\n",
    "eps = 1e-7\n",
    "\n",
    "s1 = lambda x: torch.sqrt(torch.var(x+eps))\n",
    "s2 = lambda x: torch.var(x+eps)\n",
    "\n",
    "for s in [s1,s2]:\n",
    "    x = torch.tensor([4,4.,4.,4.01], requires_grad=True)\n",
    "\n",
    "    loss = s(x)\n",
    "\n",
    "    loss.backward()\n",
    "    print(round(float(loss),2), x.grad)"
   ]
